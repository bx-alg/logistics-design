0%\" :xData=\"lineDataTwo.xData\" :yData=\"lineDataTwo.yData\" yName=\"件\" itemColor=\"#5470c6\"/>\n            </div>\n          </el-card>\n        </el-col>\n        <el-col :span=\"6\">\n          <el-card class=\"box-card\" shadow=\"never\">\n            <div class=\"card-title\">近7日移库</div>\n            <div style=\"height: calc(100% - 30px);\">\n              <TrendLineChart height=\"100%\" :xData=\"lineDataThree.xData\" :yData=\"lineDataThree.yData\" yName=\"件\" itemColor=\"#c58bea\"/>\n            </div>\n          </el-card>\n        </el-col>\n        <el-col :span=\"6\">\n          <el-card class=\"box-card\" shadow=\"never\">\n            <div class=\"card-title\">近7日退货入库</div>\n            <div style=\"height: calc(100% - 30px);\">\n              <TrendLineChart height=\"100%\" yName=\"件\" :xData=\"lineDataFour.xData\" :yData=\"lineDataFour.yData\" itemColor=\"#c7a428\"/>\n            </div>\n          </el-card>\n        </el-col>\n      </el-row>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport CirclePieChart from './components/CirclePieChart.vue'\nimport TrendLineChart from './components/TrendLineChart.vue'\nimport BarChart from './components/BarChart.vue'\nimport { onMounted, ref, getCurrentInstance } from 'vue'\nimport moment from 'moment';\n\nconst tabPosition = ref('month')\nconst barChartData = ref({\n  yData: [79, 68, 56, 72, 51, 63, 67, 71, 58, 81, 64, 77, 56, 69]\n})\nconst barXName = ref('日')\nconst lineDataOne = ref({\n  yData: [79, 65, 21, 67, 21, 89, 56],\n})\nconst lineDataTwo = ref({\n  yData: [45, 72, 16, 37, 64, 28, 46],\n})\nconst lineDataThree = ref(\n  {\n    yData: [16, 27, 37, 16, 27, 21, 11],\n  }\n)\nconst lineDataFour = ref({\n  yData: [134, 107, 94, 173, 37, 143, 86],\n})\n\n// 饼图数据\nconst pieData = ref([\n  { value: 23, name: \"园区仓\", itemStyle: { color: '#5470c6' } },\n  { value: 11, name: \"盛泽仓\", itemStyle: { color: '#91cc75' } },\n  { value: 5, name: \"常熟仓\", itemStyle: { color: '#fac858' } },\n  { value: 0, name: \"吴江仓\", itemStyle: { color: '#ee6666' } },\n  { value: 0, name: \"南京仓\", itemStyle: { color: '#73c0de' } },\n])\n\nconst router = getCurrentInstance().proxy.$router;\n\nonMounted((()=>{\n  initTime();\n  dateChange('month');\n}))\n\n// 时间类型选择\nfunction dateChange(value) {\n  let date = new Date()\n  let month = date.getMonth() + 1\n  let day = date.getDate()\n  let barXData = []\n  let barYData = []\n  if(value === 'year') {\n    for(let i = 0; i < month; i++) {\n      barXData.push(moment().subtract(i, 'months').format('YYYY-MM'))\n      barYData.push(Math.floor(Math.random()*(180-120+1))+120)\n    }\n    barXName.value = '月'\n  } else {\n    for(let i = 0; i < day; i++) {\n      barXData.push(moment().subtract(i, 'days').format('MM-DD'))\n      barYData.push(Math.floor(Math.random()*(30-15+1))+15)\n    }\n    barXName.value = '日'\n  }\n  barChartData.value = {\n    xData: barXData.reverse(),\n    yData: barYData\n  }\n}\n\n// 初始化时间模拟数据\nfunction initTime() {\n  let lineXData = []\n  for(let i = 0; i < 7; i++) {\n    lineXData.push(moment().subtract(i, 'days').format('MM-DD'))\n  }\n  lineXData = lineXData.reverse()\n  lineDataOne.value.xData = lineXData\n  lineDataTwo.value.xData = lineXData\n  lineDataThree.value.xData = lineXData\n  lineDataFour.value.xData = lineXData\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.home {\n  padding: 10px;\n  background-color: #f5f7f9;\n  height: calc(100vh - 50px);\n  overflow: auto;\n  .station-top {\n    margin-bottom: 10px;\n    .top-item-box {\n      height: 120px;\n      border-radius: 4px;\n      padding: 15px;\n      color: #fff;\n      box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\n    }\n    .item-box-one {\n      background: linear-gradient(to right, #36d1dc, #5b86e5);\n    }\n    .item-box-two {\n      background: linear-gradient(to right, #ff9966, #ff5e62);\n    }\n    .item-box-three {\n      background: linear-gradient(to right, #834d9b, #d04ed6);\n    }\n    .item-box-four {\n      background: linear-gradient(to right, #2193b0, #6dd5ed);\n    }\n  }\n  .station-middle {\n    margin-bottom: 10px;\n  }\n  .station-bottom {\n    margin-bottom: 10px;\n  }\n  .box-card {\n    height: 300px;\n    margin-bottom: 10px;\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\n  }\n  .card-title {\n    position: relative;\n    margin-bottom: 10px;\n    padding-left: 10px;\n    border-left: 4px solid #409eff;\n    font-size: 16px;\n    line-height: 20px;\n    font-weight: bold;\n  }\n}\n</style>"]}]}