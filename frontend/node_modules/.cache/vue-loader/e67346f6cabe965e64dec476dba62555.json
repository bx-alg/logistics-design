期\" value-format=\"yyyy-MM-dd\" class=\"filter-item date-range\" />\n      <el-button type=\"primary\" icon=\"el-icon-search\" class=\"filter-item\" @click=\"handleFilter\">搜索</el-button>\n      <el-button type=\"primary\" icon=\"el-icon-plus\" class=\"filter-item\" style=\"margin-left: 10px;\" @click=\"handleCreate\">新增</el-button>\n    </div>\n\n    <el-table :data=\"list\" border style=\"width: 100%\" v-loading=\"listLoading\">\n      <el-table-column prop=\"orderNo\" label=\"订单编号\" align=\"center\" width=\"180\" />\n      <el-table-column prop=\"customerName\" label=\"客户名称\" align=\"center\" width=\"180\" />\n      <el-table-column prop=\"totalAmount\" label=\"订单金额\" align=\"center\" width=\"120\">\n        <template slot-scope=\"scope\">\n          ¥ {{ scope.row.totalAmount }}\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"status\" label=\"订单状态\" align=\"center\" width=\"120\">\n        <template slot-scope=\"scope\">\n          <el-tag :type=\"scope.row.status | statusFilter\">\n            {{ scope.row.statusName }}\n          </el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"expectedDeliveryDate\" label=\"预计交付日期\" align=\"center\" width=\"150\" />\n      <el-table-column prop=\"createTime\" label=\"创建时间\" align=\"center\" width=\"180\" />\n      <el-table-column label=\"操作\" align=\"center\" fixed=\"right\" width=\"200\">\n        <template slot-scope=\"scope\">\n          <el-button size=\"mini\" type=\"primary\" @click=\"handleDetail(scope.row)\">查看</el-button>\n          <el-button size=\"mini\" type=\"warning\" @click=\"handleEdit(scope.row)\" :disabled=\"scope.row.status === 4 || scope.row.status === 5\">编辑</el-button>\n          <el-button size=\"mini\" type=\"danger\" @click=\"handleDelete(scope.row)\" :disabled=\"scope.row.status !== 0\">删除</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <el-pagination\n      background\n      @size-change=\"handleSizeChange\"\n      @current-change=\"handleCurrentChange\"\n      :current-page=\"listQuery.pageNum\"\n      :page-sizes=\"[10, 20, 30, 50]\"\n      :page-size=\"listQuery.pageSize\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      :total=\"total\"\n      style=\"margin-top: 20px; text-align: right;\"\n    />\n  </div>\n</template>\n\n<script>\n// 移除API导入\n// import { getOrderList, deleteOrder } from '@/api/order'\n\nexport default {\n  name: 'OrderList',\n  filters: {\n    statusFilter(status) {\n      const statusMap = {\n        0: 'info',\n        1: 'primary',\n        2: 'warning',\n        3: 'success',\n        4: 'success',\n        5: 'danger'\n      }\n      return statusMap[status]\n    }\n  },\n  data() {\n    return {\n      list: [],\n      total: 0,\n      listLoading: false,\n      listQuery: {\n        pageNum: 1,\n        pageSize: 10,\n        orderNo: '',\n        status: '',\n        startDate: '',\n        endDate: ''\n      },\n      dateRange: [],\n      statusOptions: [\n        { label: '待确认', value: 0 },\n        { label: '已确认', value: 1 },\n        { label: '生产中', value: 2 },\n        { label: '已发货', value: 3 },\n        { label: '已完成', value: 4 },\n        { label: '已取消', value: 5 }\n      ],\n      // 模拟订单数据\n      mockOrderList: [\n        {\n          id: 1,\n          orderNo: 'DD20230501001',\n          customerName: '上海某科技有限公司',\n          totalAmount: 128500.00,\n          status: 2,\n          statusName: '生产中',\n          expectedDeliveryDate: '2023-05-15',\n          createTime: '2023-05-01 10:30:25',\n          productionProgress: 65,\n          address: '上海市浦东新区张江高科技园区',\n          contactPerson: '张经理',\n          contactPhone: '13812345678'\n        },\n        {\n          id: 2,\n          orderNo: 'DD20230502002',\n          customerName: '北京某贸易有限公司',\n          totalAmount: 75000.00,\n          status: 1,\n          statusName: '已确认',\n          expectedDeliveryDate: '2023-05-20',\n          createTime: '2023-05-02 14:15:32',\n          productionProgress: 0,\n          address: '北京市海淀区中关村南大街5号',\n          contactPerson: '李总',\n          contactPhone: '13912345678'\n        },\n        {\n          id: 3,\n          orderNo: 'DD20230428003',\n          customerName: '广州某电子科技公司',\n          totalAmount: 56800.00,\n          status: 3,\n          statusName: '已发货',\n          expectedDeliveryDate: '2023-05-08',\n          createTime: '2023-04-28 09:20:45',\n          productionProgress: 100,\n          address: '广州市天河区珠江新城冼村路12号',\n          contactPerson: '王经理',\n          contactPhone: '13612345678'\n        },\n        {\n          id: 4,\n          orderNo: 'DD20230425004',\n          customerName: '深圳某科技有限公司',\n          totalAmount: 96000.00,\n          status: 4,\n          statusName: '已完成',\n          expectedDeliveryDate: '2023-05-05',\n          createTime: '2023-04-25 16:40:12',\n          productionProgress: 100,\n          address: '深圳市南山区科技园南区',\n          contactPerson: '陈经理',\n          contactPhone: '13312345678'\n        },\n        {\n          id: 5,\n          orderNo: 'DD20230503005',\n          customerName: '成都某机械有限公司',\n          totalAmount: 42500.00,\n          status: 0,\n          statusName: '待确认',\n          expectedDeliveryDate: '2023-05-25',\n          createTime: '2023-05-03 11:10:08',\n          productionProgress: 0,\n          address: '成都市高新区天府大道1388号',\n          contactPerson: '周经理',\n          contactPhone: '13512345678'\n        },\n        {\n          id: 6,\n          orderNo: 'DD20230504006',\n          customerName: '杭州某电子商务公司',\n          totalAmount: 35800.00,\n          status: 5,\n          statusName: '已取消',\n          expectedDeliveryDate: '2023-05-18',\n          createTime: '2023-05-04 08:25:38',\n          productionProgress: 0,\n          address: '杭州市西湖区文三路478号',\n          contactPerson: '林经理',\n          contactPhone: '13712345678'\n        },\n        {\n          id: 7,\n          orderNo: 'DD20230430007',\n          customerName: '武汉某科教设备公司',\n          totalAmount: 68200.00,\n          status: 1,\n          statusName: '已确认',\n          expectedDeliveryDate: '2023-05-22',\n          createTime: '2023-04-30 15:35:42',\n          productionProgress: 0,\n          address: '武汉市洪山区光谷大道62号',\n          contactPerson: '刘经理',\n          contactPhone: '13812345679'\n        },\n        {\n          id: 8,\n          orderNo: 'DD20230429008',\n          customerName: '南京某医疗器械公司',\n          totalAmount: 125000.00,\n          status: 2,\n          statusName: '生产中',\n          expectedDeliveryDate: '2023-05-12',\n          createTime: '2023-04-29 13:50:15',\n          productionProgress: 45,\n          address: '南京市江宁区将军大道100号',\n          contactPerson: '吴总',\n          contactPhone: '13912345679'\n        },\n        {\n          id: 9,\n          orderNo: 'DD20230427009',\n          customerName: '天津某机电有限公司',\n          totalAmount: 89600.00,\n          status: 3,\n          statusName: '已发货',\n          expectedDeliveryDate: '2023-05-07',\n          createTime: '2023-04-27 10:45:20',\n          productionProgress: 100,\n          address: '天津市西青区大学城学府路80号',\n          contactPerson: '赵经理',\n          contactPhone: '13612345679'\n        },\n        {\n          id: 10,\n          orderNo: 'DD20230505010',\n          customerName: '重庆某汽车配件厂',\n          totalAmount: 105300.00,\n          status: 0,\n          statusName: '待确认',\n          expectedDeliveryDate: '2023-05-30',\n          createTime: '2023-05-05 09:15:30',\n          productionProgress: 0,\n          address: '重庆市渝北区回兴街道宝环路40号',\n          contactPerson: '钱经理',\n          contactPhone: '13312345679'\n        }\n      ]\n    }\n  },\n  created() {\n    this.getList()\n  },\n  watch: {\n    dateRange(val) {\n      if (val && val.length === 2) {\n        this.listQuery.startDate = val[0]\n        this.listQuery.endDate = val[1]\n      } else {\n        this.listQuery.startDate = ''\n        this.listQuery.endDate = ''\n      }\n    }\n  },\n  methods: {\n    handleFilter() {\n      this.listQuery.pageNum = 1\n      this.getList()\n    },\n    getList() {\n      this.listLoading = true\n      \n      // 使用模拟数据代替API调用\n      setTimeout(() => {\n        let filteredList = this.mockOrderList\n\n        // 根据查询条件筛选数据\n        if (this.listQuery.orderNo) {\n          filteredList = filteredList.filter(item => \n            item.orderNo.includes(this.listQuery.orderNo)\n          )\n        }\n        \n        if (this.listQuery.status !== '') {\n          filteredList = filteredList.filter(item => \n            item.status === this.listQuery.status\n          )\n        }\n        \n        if (this.listQuery.startDate && this.listQuery.endDate) {\n          filteredList = filteredList.filter(item => {\n            const createDate = item.createTime.split(' ')[0]\n            return createDate >= this.listQuery.startDate && createDate <= this.listQuery.endDate\n          })\n        }\n        \n        // 分页处理\n        const start = (this.listQuery.pageNum - 1) * this.listQuery.pageSize\n        const end = start + this.listQuery.pageSize\n        \n        this.total = filteredList.length\n        this.list = filteredList.slice(start, end)\n        this.listLoading = false\n      }, 500) // 模拟网络延迟\n    },\n    handleSizeChange(val) {\n      this.listQuery.pageSize = val\n      this.getList()\n    },\n    handleCurrentChange(val) {\n      this.listQuery.pageNum = val\n      this.getList()\n    },\n    handleCreate() {\n      this.$router.push({\n        name: 'OrderCreate'\n      })\n    },\n    handleEdit(row) {\n      this.$router.push({\n        name: 'OrderDetail',\n        params: { id: row.id }\n      })\n    },\n    handleDetail(row) {\n      this.$router.push({\n        name: 'OrderDetail',\n        params: { id: row.id }\n      })\n    },\n    handleDelete(row) {\n      this.$confirm('确认删除该订单吗?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        // 模拟删除操作\n        const index = this.mockOrderList.findIndex(item => item.id === row.id)\n        if (index !== -1) {\n          this.mockOrderList.splice(index, 1)\n          this.$message.success('删除成功')\n          this.getList()\n        }\n      }).catch(() => {})\n    }\n  }\n}\n</script>\n\n<style scoped>\n.filter-container {\n  padding-bottom: 10px;\n}\n.filter-item {\n  margin-right: 10px;\n  margin-bottom: 10px;\n}\n.date-range {\n  width: 350px;\n}\n</style> "]}]}